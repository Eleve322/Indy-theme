eToggle() {\n    const wrap = document.getElementById('auth-wrapper');\n    const btn = document.getElementById('auth-btn');\n    \n    if (wrap.classList.contains('open')) {\n      wrap.classList.remove('open');\n      btn.innerText = 'Ver especificaciones de {{ collection.title }}';\n    } else {\n      wrap.classList.add('open');\n      btn.innerText = 'Cerrar detalles';\n    }\n  }\n<\/script>","padding_top":0,"padding_bottom":0,"reset_spacing":false}}},"order":["ad5b88b9-7c73-40d8-a7e0-60b6519d3f30","b95bf1ad-ba57-434a-bbe3-b729443926a1","custom_liquid_RwcXnM","collection_list_m7FbBB","main-collection-heading","439e38df-206c-40a8-9cd5-fc0e5f00f9d3","product-grid","sub-collection","faq_e3imzr","faq_Wk3myd","custom_liquid_4fFbqp"]}